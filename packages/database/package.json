{
    "name": "@repo/db",
    "version": "0.0.0",
    "devDependencies": {
        "prisma": "^6.5.0",
        "ts-node": "^10.9.2"
    },
    "dependencies": {
        "@prisma/client": "^6.5.0"
    },
    "scripts": {
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev --skip-generate",
        "db:deploy": "prisma migrate deploy"
    },
    "exports": {
        ".": "./src/index.ts"
    },
    "prisma": {
        "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    }
}